let
  var a: Integer;
  var b: Integer
in
  begin
    getint (var a);
    getint (var b);
    if a = b then
      begin
        put ('S'); put ('a'); put ('m'); put ('e'); put ('!');
      end
    else
      begin
        putint (a); put ('+'); putint (b); put ('='); putint (a + b);
         puteol();
        putint (a); put ('*'); putint (b); put ('='); putint (a * b);
      end
  end
